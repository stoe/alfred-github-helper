{
  "name": "@stoe/alfred-github-helper",
  "version": "1.0.6",
  "description": "Alfred GitHub",
  "keywords": [
    "alfred",
    "github",
    "bookmarks",
    "teams",
    "members"
  ],
  "author": {
    "name": "Stefan StÃ¶lzle",
    "email": "stoe@github.com",
    "url": "https://github.com/stoe"
  },
  "license": "MIT",
  "homepage": "https://github.com/stoe/alfred-github-helper#readme",
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com/stoe/alfred-github-helper.git"
  },
  "bugs": {
    "url": "https://github.com/stoe/alfred-github-helper/issues"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "engines": {
    "node": ">=12",
    "npm": ">=6"
  },
  "directories": {
    "lib": "lib"
  },
  "files": [
    "assets/",
    "lib/",
    "icon.png",
    "info.plist",
    "license",
    "package.json",
    "readme.md"
  ],
  "scripts": {
    "postinstall": "alfy-init",
    "preuninstall": "alfy-cleanup",
    "pretest": "node node_modules/.bin/eslint-config-prettier .eslintrc.json",
    "test": "eslint **/*.js && ava"
  },
  "dependencies": {
    "@octokit/core": "^3.2.5",
    "alfy": "^0.10.0",
    "open": "^7.3.1"
  },
  "devDependencies": {
    "@github/prettier-config": "^0.0.4",
    "alfy-test": "^0.4.0",
    "ava": "^3.15.0",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-github": "^4.1.1"
  },
  "resolutions": {
    "dot-prop": ">=5.2.0"
  },
  "ava": {
    "files": [
      "**/*.t.js"
    ],
    "verbose": true
  },
  "scope": [
    "repo",
    "read:user",
    "read:org",
    "user:email"
  ],
  "prettier": "@github/prettier-config"
}
