{
  "name": "@stoe/alfred-github-helper",
  "version": "1.2.0",
  "type": "module",
  "description": "Alfred GitHub",
  "keywords": [
    "alfred",
    "github",
    "bookmarks",
    "teams",
    "members"
  ],
  "author": {
    "name": "Stefan StÃ¶lzle",
    "email": "stoe@github.com",
    "url": "https://github.com/stoe"
  },
  "license": "MIT",
  "repository": "github:stoe/alfred-github-helper",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "engines": {
    "node": ">=14",
    "npm": ">=7"
  },
  "directories": {
    "lib": "lib"
  },
  "files": [
    "assets/",
    "lib/",
    "icon.png",
    "info.plist",
    "license",
    "package.json",
    "readme.md"
  ],
  "scripts": {
    "format": "npx prettier --config-precedence prefer-file --write . && eslint . --fix",
    "postinstall": "alfy-init",
    "preuninstall": "alfy-cleanup",
    "prepare": "husky install ./.github/husky",
    "pretest": "npx eslint-config-prettier .eslintrc.json",
    "test": "eslint **/*.js && ava"
  },
  "dependencies": {
    "@octokit/core": "^3.5.1",
    "alfy": "^0.12.1",
    "open": "^8.4.0",
    "read-pkg-up": "^9.0.0"
  },
  "devDependencies": {
    "@github/prettier-config": "^0.0.4",
    "alfy-test": "^0.4.2",
    "ava": "^3.15.0",
    "eslint": "^8.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-github": "^4.3.3"
  },
  "ava": {
    "files": [
      "**/tests/*.js"
    ],
    "verbose": true
  },
  "prettier": "@github/prettier-config",
  "scope": [
    "repo",
    "read:user",
    "read:org",
    "user:email"
  ]
}
